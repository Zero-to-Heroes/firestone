{
	"name": "firestone",
	"version": "9.12.0",
	"scripts": {
		"lint": "node --max_old_space_size=10000 ./node_modules//eslint/bin/eslint.js --color --fix --ext .ts ./src/",
		"test": "node --max_old_space_size=10000 ./node_modules/jest/bin/jest.js --colors",
		"singleTest": "node --max_old_space_size=10000 ./node_modules/jest/bin/jest.js --runInBand -t",
		"singleFile": "node --max_old_space_size=10000 ./node_modules/jest/bin/jest.js --runInBand",
		"remove_evil:sentry-types": "rimraf node_modules/@sentry/integrations/node_modules/@sentry/types",
		"remove_evil:node_modules": "rimraf node_modules/@firestone-hs/**/node_modules",
		"dev": "npm run postinstall && node --max_old_space_size=10000 ./node_modules/webpack/bin/webpack.js --config webpack.config.js --env.appversion=%npm_package_version% --env.development --progress --profile --bail --colors --watch",
		"dev:clean": "rimraf dist && node --max_old_space_size=10000 ./node_modules/webpack/bin/webpack.js --config webpack.config.js --env.appversion=%npm_package_version% --env.development --progress --profile --bail --colors",
		"local-test": "rimraf dist  && node --max_old_space_size=10000 ./node_modules/webpack/bin/webpack.js --config webpack.config.js --env.appversion=%npm_package_version% --env.localTest --progress --profile --bail --colors",
		"comment": "the zipbuild script has been modified manually because the % character does some funky stuff on windows",
		"zipdist": "node ./node_modules/zip-build/src/index.js dist . -t @@NAME@@_@@VERSION@@.opk",
		"build": "rimraf dist && npm audit fix && npm run lint && npm run remove_evil:node_modules && node --max_old_space_size=10000 ./node_modules/webpack/bin/webpack.js --config webpack.config.js --env.appversion=%npm_package_version% --env.production --progress --profile --bail --colors && npm run zipdist",
		"justbuild": "node --max_old_space_size=10000 ./node_modules/webpack/bin/webpack.js --config webpack.config.js --env.appversion=%npm_package_version% --env.production --progress --profile --bail --colors && npm run zipdist",
		"postinstall": "npm run remove_evil:sentry-types && npm run remove_evil:node_modules",
		"dev-twitch": "npm run postinstall && rimraf dist-twitch && node --max_old_space_size=10000 ./node_modules/webpack/bin/webpack.js --config webpack-twitch.config.js --env.appversion=%npm_package_version% --env.development --progress --profile --bail --colors --watch",
		"build-twitch": "rimraf dist-twitch && node --max_old_space_size=10000 ./node_modules/webpack/bin/webpack.js --config webpack-twitch.config.js --env.appversion=%npm_package_version% --env.production --progress --profile --bail --colors"
	},
