{
	"name": "firestone",
	"version": "4.0.0",
	"scripts": {
		"lint": "node --max_old_space_size=6000 ./node_modules//eslint/bin/eslint.js --color --fix --ext .ts ./src/",
		"test": "node --max_old_space_size=6000 ./node_modules/jest/bin/jest.js --colors",
		"singleTest": "node --max_old_space_size=6000 ./node_modules/jest/bin/jest.js --runInBand -t",
		"remove_evil:sentry-types": "rimraf node_modules/@sentry/integrations/node_modules/@sentry/types",
		"dev": "node --max_old_space_size=6000 ./node_modules/webpack/bin/webpack.js --config webpack.config.js --env.appversion=%npm_package_version% --env.development --progress --profile --bail --colors",
		"dev:clean": "rimraf dist && node --max_old_space_size=6000 ./node_modules/webpack/bin/webpack.js --config webpack.config.js --env.appversion=%npm_package_version% --env.development --progress --profile --bail --colors",
		"local-test": "rimraf dist && node --max_old_space_size=6000 ./node_modules/webpack/bin/webpack.js --watch --config webpack.config.js --env.appversion=%npm_package_version% --env.localTest --progress --profile --bail --colors",
		"build": "rimraf dist && npm audit fix && npm run lint && npm run test && node --max_old_space_size=6000 ./node_modules/webpack/bin/webpack.js --config webpack.config.js --env.appversion=%npm_package_version% --env.production --progress --profile --bail --colors",
		"postinstall": "npm run remove_evil:sentry-types",
		"dev-twitch": "rimraf dist-twitch && node --max_old_space_size=6000 ./node_modules/webpack/bin/webpack.js --watch --config webpack-twitch.config.js --env.appversion=%npm_package_version% --env.development --progress --profile --bail --colors",
		"build-twitch": "rimraf dist-twitch && npm audit fix && node --max_old_space_size=6000 ./node_modules/webpack/bin/webpack.js --config webpack-twitch.config.js --env.appversion=%npm_package_version% --env.production --progress --profile --bail --colors"
	},
	"license": "MIT",
	"dependencies": {
		"@angular/animations": "8.1.1",
		"@angular/cdk": "8.1.0",
		"@angular/common": "8.1.1",
		"@angular/compiler": "8.1.1",
		"@angular/core": "8.1.1",
		"@angular/forms": "8.1.1",
		"@angular/material": "8.1.0",
		"@angular/platform-browser": "8.1.1",
		"@angular/platform-browser-dynamic": "8.1.1",
		"@firestone-hs/reference-data": "^0.1.0",
		"@sentry/browser": "^5.7.1",
