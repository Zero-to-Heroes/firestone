		"copy-webpack-plugin": "4.5.2",
		"css-loader": "3.2.0",
		"css-to-string-loader": "^0.1.3",
		"eslint": "6.1.0",
		"eslint-config-prettier": "6.0.0",
		"eslint-loader": "2.2.1",
		"eslint-plugin-prettier": "3.1.0",
